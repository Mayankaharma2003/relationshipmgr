<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-3.xsd">

    <service verb="create" noun="RoleType" type="inline" >
        <in-parameters>
            <auto-parameters required="true" include="all"/>
        </in-parameters>
        <out-parameters>
            <parameter name="roleTypeId" required="true"/>
        </out-parameters>
        <actions>
            <entity-make-value entity-name="RoleType" value-field="newRole"/>
            <entity-set value-field="newRole" include="all"/>
            <entity-create value-field="newRole" />
        </actions>
    </service>
</services>